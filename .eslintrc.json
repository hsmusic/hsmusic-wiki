{
  "env": {
    "es2021": true,
    "node": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "indent": ["off"],
    "no-unused-vars": ["error", {
      "argsIgnorePattern": "^_",
      "destructuredArrayIgnorePattern": "^"
    }]
  }
}
